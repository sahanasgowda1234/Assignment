    "# Answer\n",
    "num= int(input(\"total numbers you want in the series:\"))\n",
    "n1=0\n",
    "n2=1\n",
    "for kk in range(num):\n",
    "    save= n1\n",
    "    print(n1, end=\",\")\n",
    "    n1= n2\n",
    "    n2= save+ n2\n",
    "    if n1>50:\n",
    "        break\n",
